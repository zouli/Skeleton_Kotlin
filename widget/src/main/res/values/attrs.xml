<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckableLinearLayout">
        <attr name="cll_childCheckable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BoxCaptchaView">
        <attr name="bcv_charNumber" format="integer" />
        <attr name="bcv_divideWidth" format="dimension" />
        <attr name="bcv_itemStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="RefreshListView">
        <attr name="rlv_direction">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="both" value="2" />
        </attr>
        <attr name="rlv_divider" format="reference" />
        <attr name="rlv_dividerHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RefreshCompleteListView">
        <attr name="rclv_direction">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="both" value="2" />
        </attr>
        <attr name="rclv_divider" format="reference" />
        <attr name="rclv_dividerHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RefreshGridView">
        <attr name="rgv_direction">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="both" value="2" />
        </attr>
        <attr name="rgv_numColumns" format="integer" />
        <attr name="rgv_horizontalSpacing" format="dimension" />
        <attr name="rgv_verticalSpacing" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CompleteListView">
        <attr name="clv_divider" format="reference" />
        <attr name="clv_dividerHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ImageGridView">
        <attr name="igv_readOnly" format="boolean" />
        <attr name="igv_smartColumnCount" format="boolean" />
        <attr name="igv_addButtonIcon" format="reference" />
        <attr name="igv_deleteButtonIcon" format="reference" />
        <attr name="igv_imageCount" format="integer" />
        <attr name="igv_dividerSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SearchBar">
        <attr name="sb_logo" format="reference" />
        <attr name="sb_hint" format="reference|string" />
        <attr name="sb_buttonVisibility" format="boolean" />
        <attr name="sb_isFocusable" format="boolean" />
        <attr name="sb_cancelButton" format="reference|string" />
        <attr name="sb_searchButton" format="reference|string" />
        <attr name="sb_editTextMarginStart" format="dimension" />
    </declare-styleable>

    <attr name="searchBarStyle" format="reference" />
    <attr name="searchBarEditTextStyle" format="reference" />
    <attr name="searchBarButtonStyle" format="reference" />

    <declare-styleable name="PairView">
        <attr name="pv_firstViewStyle" format="reference" />
        <attr name="pv_secondViewStyle" format="reference" />
        <attr name="pv_firstImage" format="reference" />
        <attr name="pv_secondImage" format="reference" />
        <attr name="pv_firstImageSize" format="dimension" />
        <attr name="pv_secondImageSize" format="dimension" />
        <attr name="pv_firstText" format="reference|string" />
        <attr name="pv_secondText" format="reference|string" />
    </declare-styleable>

    <attr name="icv_textHint" format="reference|string" />
    <attr name="icv_maxLength" format="integer" />
    <attr name="icv_editPaddingStart" format="dimension" />
    <attr name="icv_editPaddingEnd" format="dimension" />
    <attr name="icv_editPaddingTop" format="dimension" />
    <attr name="icv_editPaddingBottom" format="dimension" />

    <declare-styleable name="InputCaptchaView">
        <attr name="icv_textHint" />
        <attr name="icv_maxLength" />
        <attr name="icv_phoneNumberId" format="reference" />
        <attr name="icv_sleepSecond" format="integer" />
        <attr name="icv_editPaddingStart" />
        <attr name="icv_editPaddingEnd" />
        <attr name="icv_editPaddingTop" />
        <attr name="icv_editPaddingBottom" />
    </declare-styleable>

    <declare-styleable name="ImageCaptchaView">
        <attr name="icv_textHint" />
        <attr name="icv_maxLength" />
        <attr name="icv_editTextMarginEnd" format="dimension" />
        <attr name="icv_editBackground" format="reference" />
        <attr name="icv_editPaddingStart" />
        <attr name="icv_editPaddingEnd" />
        <attr name="icv_editPaddingTop" />
        <attr name="icv_editPaddingBottom" />
    </declare-styleable>
</resources>