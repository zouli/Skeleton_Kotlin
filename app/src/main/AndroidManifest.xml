        <activity
                android:name=".widgettest.anko.SearchBarActivity"
                android:screenOrientation="portrait" />
        <activity
                android:name=".widgettest.xml.PairViewActivity"
                android:screenOrientation="portrait" />
        <activity
                android:name=".widgettest.anko.PairViewActivity"
                android:screenOrientation="portrait" />

        <service android:name=".basetest.TestService" />
    </application>
</manifest>
